{
  "name": "master-claude-system",
  "version": "2.0.0",
  "description": "Master Claude AI Farm Management System with Supabase SQL Executor",
  "type": "module",
  "main": "launch.py",
  "scripts": {
    "sql:setup": "node supabase-sql-executor.js setup",
    "sql:test": "node supabase-sql-executor.js test",
    "sql:file": "node supabase-sql-executor.js file",
    "sql:query": "node supabase-sql-executor.js query",
    "sql:research": "node supabase-sql-executor.js research",
    "sql:list": "node supabase-sql-executor.js list",
    "launch": "python3 launch.py",
    "monitor": "python3 check-mycelial-network.py",
    "respond": "python3 respond-to-network.py",
    "start-faith": "cd projects/faith && python3 -c \"import time; import subprocess; subprocess.run(['claude', 'Read CLAUDE.md and execute monitoring duties'], check=False)\"",
    "check-all": "npm run monitor && npm run respond",
    "deploy-sql": "npm run sql:setup && npm run sql:research",
    "setup-webbuilder-comm": "node setup-aidan-webbuilder-communication.js",
    "check-webbuilder": "node check-aidan-webbuilder-projects.js",
    "test-webbuilder-comm": "node setup-aidan-webbuilder-communication.js test"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.49.8",
    "dotenv": "^16.5.0",
    "fs-extra": "^11.3.0",
    "node-fetch": "^3.3.2",
    "resend": "^4.5.1",
    "commander": "^11.1.0",
    "chalk": "^5.3.0",
    "ora": "^7.0.1"
  },
  "devDependencies": {
    "@types/node": "^20.10.4",
    "nodemon": "^3.0.2"
  },
  "keywords": [
    "ai",
    "claude",
    "supabase",
    "sql-executor",
    "farm-management",
    "automation",
    "mcp"
  ],
  "author": "Pete's AI Farm Ecosystem",
  "license": "MIT",
  "engines": {
    "node": ">=18.0.0"
  }
}
